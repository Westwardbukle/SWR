<Window x:Class="SquareAndLine.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SquareAndLine"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="98" Margin="10,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="774"/>
        <Button x:Name="SquareButton" Content="Квадратное уравнение" HorizontalAlignment="Left" Margin="22,113,0,0" VerticalAlignment="Top" Width="279" Height="276" Click="SquareButton_Click"/>
        <Button x:Name="LIneButton" Content="Линейное уравнение" HorizontalAlignment="Left" Margin="498,113,0,0" VerticalAlignment="Top" Width="286" Height="276" Click="LIneButton_Click"/>
        <Label Content="Вычисление корней " HorizontalAlignment="Left" Margin="596,25,0,0" VerticalAlignment="Top" Height="65" Width="188" FontSize="18"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="10,8,0,0" VerticalAlignment="Top" Width="140"/>
    </Grid>
</Window>
